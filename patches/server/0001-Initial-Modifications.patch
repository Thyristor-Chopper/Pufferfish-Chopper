+
+public class TestForCommand {
+    @SuppressWarnings({ "unchecked", "rawtypes" })
+    public static void register(CommandDispatcher<CommandSourceStack> commandDispatcher) {
+        commandDispatcher.register((LiteralArgumentBuilder) ((LiteralArgumentBuilder) ((LiteralArgumentBuilder) Commands.literal("testfor")
+            .requires(commandSourceStack -> commandSourceStack.hasPermission(1)))
+            .then((((RequiredArgumentBuilder) Commands.argument("targets", EntityArgument.entities()))
+                .executes(commandContext -> TestForCommand.testFor((CommandSourceStack) commandContext.getSource(), EntityArgument.getEntities((CommandContext<CommandSourceStack>) commandContext, "targets"), new CompoundTag())))
+            .then(Commands.argument("nbt", CompoundTagArgument.compoundTag())
+                .executes(commandContext -> TestForCommand.testFor((CommandSourceStack)commandContext.getSource(), EntityArgument.getEntities((CommandContext<CommandSourceStack>) commandContext, "targets"), CompoundTagArgument.getCompoundTag(commandContext, "nbt")))))));
+    }
+
+    private static int testFor(CommandSourceStack commandSourceStack, Collection<? extends Entity> collection, CompoundTag nbt) throws CommandSyntaxException {
+        CommandSender sender = commandSourceStack.getBukkitSender();
+        String success = "%s을(를) 찾았습니다";
+        String fail = "%s이(가) 필요한 데이터 구조와 일치하지 않습니다";
+        /*
+        String success = "commands.testfor.success";
+        String fail = "commands.testfor.failure";
+        if(sender instanceof Player) {
+            Player player = (Player) sender;
+            String locale = player.getLocale();
+            if(locale.equalsIgnoreCase("ko_kr")) {
+                success = "%s을(를) 찾았습니다";
+                fail = "%s이(가) 필요한 데이터 구조와 일치하지 않습니다";
+            } else if(locale.equalsIgnoreCase("en_us")) {
+                success = "Found %s";
+                fail = "%s did not match the required data structure";
+            }
+        }
+        */
+        int n = 0;
+        for (Entity entity : collection) {
+            EntityDataAccessor dataAccessor = new EntityDataAccessor(entity);
+            CompoundTag data = dataAccessor.getData();
+            for(String key : nbt.getAllKeys()) {
+                if(!data.contains(key) || !data.get(key).equals(nbt.get(key)))
+                    throw new SimpleCommandExceptionType((Message) new TranslatableComponent(fail, entity.getName())).create();
+            }
+            ++n;
+            commandSourceStack.sendSuccess(new TranslatableComponent(success, entity.getName()), false);
+        }
+        return n;
+    }
+}
