+package net.minecraft.server.commands;
+
+import com.google.common.collect.ImmutableList;
+import com.mojang.brigadier.CommandDispatcher;
+import java.util.Collection;
+import net.minecraft.commands.CommandSourceStack;
+import net.minecraft.commands.Commands;
+import net.minecraft.commands.arguments.EntityArgument;
+import net.minecraft.network.chat.TranslatableComponent;
+import net.minecraft.world.entity.Entity;
+import net.minecraft.world.entity.decoration.*;
+import net.minecraft.world.entity.item.*;
+
+public class KillCommand {
+    public static void register(CommandDispatcher<CommandSourceStack> dispatcher) {
+        dispatcher.register(Commands.literal("kill").requires((source) -> {
+            return source.hasPermission(3);
+        }).executes((context) -> {
+            return kill(context.getSource(), ImmutableList.of(context.getSource().getEntityOrException()), false);
+        }).then(Commands.argument("targets", EntityArgument.entities()).executes((context) -> {
+            return kill(context.getSource(), EntityArgument.getEntities(context, "targets"), false);
+        }).then(Commands.literal("force").executes((context) -> {
+            return kill(context.getSource(), EntityArgument.getEntities(context, "targets"), true);
+        }))));
+    }
+
+    private static int kill(CommandSourceStack source, Collection<? extends Entity> targets, boolean force) {
+        boolean warn = false;
+        int n = 0;
+        for(Entity entity : targets) {
+            if(!force && (entity instanceof ArmorStand || entity instanceof ItemFrame || entity instanceof Painting || entity instanceof LeashFenceKnotEntity || entity instanceof HangingEntity || entity instanceof FallingBlockEntity || entity instanceof ItemEntity)) {
+                warn = true;
+                continue;
+            }
+            entity.kill();
+            n++;
+        }
+        
+        if(warn)
+            source.getBukkitSender().sendMessage("§e건축물 손상 방지를 위해 그림, 갑옷 거치대, 아이템 액자, 목줄, 폴링 블록 및 떨어진 아이템은 제거되지 않습니다. 강제로 제거하려면 /kill <대상> force를 사용하십시오");
+
+        if(n == 1) {
+            source.sendSuccess(new TranslatableComponent("%s을(를) 죽였습니다", targets.iterator().next().getDisplayName()), true);
+        } else if(warn) {
+            source.sendSuccess(new TranslatableComponent("개체 %s개를 제외하고 %s개를 죽였습니다", targets.size() - n, n), true);
+        } else {
+            source.sendSuccess(new TranslatableComponent("개체 %s개를 죽였습니다", n), true);
+        }
+
+        return n;
+    }
+}
