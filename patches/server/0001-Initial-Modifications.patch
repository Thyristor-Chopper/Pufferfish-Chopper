+    private static int setThunder(CommandSourceStack source, int duration) {
+        source.getLevel().setWeatherParameters(0, duration, true, true);
+        source.sendSuccess(new TranslatableComponent("날씨를 변경하였습니다"), true);
+        return duration;
+    }
+}
