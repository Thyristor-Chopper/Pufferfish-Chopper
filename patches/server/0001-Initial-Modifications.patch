+    public Dynamic2CommandExceptionType integerTooLow() {
+        return INTEGER_TOO_SMALL;
+    }
+
+    public Dynamic2CommandExceptionType integerTooHigh() {
+        return INTEGER_TOO_BIG;
+    }
