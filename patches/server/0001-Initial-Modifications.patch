+        return new TranslatableComponent("clear.failed.multiple", playerCount);
+    });
+
+    public static void register(CommandDispatcher<CommandSourceStack> dispatcher) {
+        dispatcher.register(Commands.literal("clear").requires((source) -> {
+            return source.hasPermission(1);
+        }).executes((context) -> {
+            return clearInventory(context.getSource(), Collections.singleton(context.getSource().getPlayerOrException()), (stack) -> {
+                return true;
+            }, -1);
+        }).then(Commands.argument("targets", EntityArgument.players()).requires((source) -> {
+            return source.hasPermission(3);
