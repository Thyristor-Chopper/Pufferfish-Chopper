+            literalArgumentBuilder.then(Commands.literal(key).executes(commandContext -> OldDifficultyCommand.setDifficulty((CommandSourceStack)commandContext.getSource(), difficulty)));
+        });
+        literalArgumentBuilder
+            .then(Commands.argument("difficulty", IntegerArgumentType.integer(0, 3))
+            .executes(commandContext -> OldDifficultyCommand.setDifficulty((CommandSourceStack)commandContext.getSource(), numericDifficulties[IntegerArgumentType.getInteger((CommandContext<CommandSourceStack>) commandContext, "difficulty")])));
